programa
{
	
real n1, n2
	real media
	funcao inicio()
	{
		escreva("Digite a primeira nota ")
		leia(n1)
		escreva("Digite a segunda nota ")
		leia(n2)

		media = (n1 + n2) / 2

		se ( media >= 7 ){
			escreva (" aprovado ")
		}
		se ( media <7 ){
			escreva (" reprovado ")
		}
		escreva (" a media foi de "+ media)

	}
}
