programa
{
	
real nota1, nota2, mediaTudo
	funcao inicio()
	{
		escreva("digite uma nota ")
		leia(nota1)
		escreva (" digite outra nota ")
		leia(nota2)
		mediaTudo =  mediAluno(nota1, nota2)
		mediaTudo =  mediAluno(nota1, nota2)
		escreva (" a media Ã© "+ mediaTudo)
		
	}

	funcao real mediAluno ( real n1, real n2)
	{	
		real media
		media = (n1 + n2 ) / 2
		retorne  media
	}
}
